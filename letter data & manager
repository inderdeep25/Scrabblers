class LetterManager{
public void int LetterData(){
int a[]{1,1,2,2,1,2,2,1,2,1,1,1,2,2,1,3,5,2,3,1,1,2,3,5,10,10,10};
char b[]{'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};
for(int i=0;i<26;i++)
{
for(int j=0;j<26;j++)
{
a[i]=b[j];
}
}
public void int ScoreOfLetter()
{
ScoreOfLetter sl=new ScoreOfLetter();
score=sl.GetScore();
return score;
}
}
}
